      <class id="subject_scope" label=""/>
      <span end="2653" id="PMC3313761.nxml.gz-22" start="2523">Higher prenatal vitamin D levels are shown to have a protective role against wheezing in young children in epidemiological studies</span>
    </annotation>
    <annotation annotator="Default" id="PMC3313761.nxml.gz-23" type="identity">
