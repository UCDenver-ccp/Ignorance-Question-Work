      <class id="subject_scope" label=""/>
      <span end="27039" id="PMC6054603.nxml.gz-142" start="26935">Previous evidence on the longer-term effects of antenatal dietary and lifestyle interventions is limited</span>
    </annotation>
    <annotation annotator="Default" id="PMC6054603.nxml.gz-143" type="identity">
      <class id="evidence...limited" label=""/>
      <span end="26952" id="PMC6054603.nxml.gz-143" start="26944">evidence</span>
