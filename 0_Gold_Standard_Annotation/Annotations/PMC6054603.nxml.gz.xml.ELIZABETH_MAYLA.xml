      <class id="while" label=""/>
    </annotation>
    <annotation annotator="Default" id="PMC6054603.nxml.gz-170" type="identity">
      <class id="subject_scope" label=""/>
      <span end="29839" id="PMC6054603.nxml.gz-170" start="29680">Our findings report, for the first time, the impact of an antenatal dietary intervention for women who are overweight or obese on early infant neurodevelopment</span>
    </annotation>
    <annotation annotator="Default" id="PMC6054603.nxml.gz-171" type="identity">
      <class id="first_time" label=""/>
      <span end="29719" id="PMC6054603.nxml.gz-171" start="29709">first time</span>
    </annotation>
    <annotation annotator="Default" id="PMC6054603.nxml.gz-172" type="identity">
      <class id="impact" label=""/>
      <span end="29731" id="PMC6054603.nxml.gz-172" start="29725">impact</span>
    </annotation>
    <annotation annotator="Default" id="PMC6054603.nxml.gz-173" type="identity">
      <class id="subject_scope" label=""/>
      <span end="30221" id="PMC6054603.nxml.gz-173" start="29841">While we did not identify statistically or clinically significant differences in neurodevelopment at 6-months of age using a reliable screening tool, ongoing follow-up of participants further into childhood will again be important to determine whether there are any potential benefits of improving maternal diet and physical activity during pregnancy on childhood neurodevelopment</span>
    </annotation>
    <annotation annotator="Default" id="PMC6054603.nxml.gz-174" type="identity">
      <class id="while" label=""/>
    </annotation>
