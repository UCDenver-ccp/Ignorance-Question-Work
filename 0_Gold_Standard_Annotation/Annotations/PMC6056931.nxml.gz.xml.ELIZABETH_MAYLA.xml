      <class id="subject_scope" label=""/>
      <span end="41731" id="PMC6056931.nxml.gz-171" start="41576">A limitation of the study is that women who fit the inclusion criteria are invited to join the study by visiting the data collection site at an agreed date</span>
    </annotation>
    <annotation annotator="Default" id="PMC6056931.nxml.gz-172" type="identity">
