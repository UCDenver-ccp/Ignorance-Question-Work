      <class id="subject_scope" label=""/>
      <span end="41217" id="PMC6056931.nxml.gz-162" start="41064">Furthermore, many healthcare facilities in Johannesburg serve a predominantly migrant population [81] posing a challenge for longitudinal data collection</span>
    </annotation>
    <annotation annotator="Default" id="PMC6056931.nxml.gz-163" type="identity">
      <class id="challenge" label=""/>
